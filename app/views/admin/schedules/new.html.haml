.adminSchedule-wrapper
  = render "shared/header-main"
  = render "shared/navbar"
  .newSchedule
    %h4.newSchedule_index
      スケジュール作成
    .newSchedule_inner
      = render "form-schedule"
      =form_with url:import_admin_schedules_path do |f|
        = f.file_field :file, accept: '.csv'
        = f.submit "アップロード"